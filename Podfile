# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'LetSinc' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for LetSinc
	pod 'IQKeyboardManagerSwift'
    pod 'JTAppleCalendar', :git => 'https://github.com/patchthecode/JTAppleCalendar.git'
    pod 'SwiftMessages'
    pod 'Firebase/Core', '~> 4.2'
    pod 'Firebase/Storage', '~> 4.2'
    pod 'Firebase/Database', '~> 4.2'
    pod 'Firebase/Auth', '~> 4.2'
    pod 'Firebase/Crash', '~> 4.2'
    pod 'Firebase/Messaging', '~> 4.2'
    pod 'SVProgressHUD'
    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    pod 'GoogleMaps'
    pod 'PhoneNumberKit'
    pod 'ActiveLabel'
    pod 'TTSegmentedControl', '~> 0.3'
    pod 'Alamofire', '~> 4.5'
end
